{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Reflect New Tab",
  "version": "1.1.0",
  "description": "",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "history",
    "identity",
    "alarms",
    "notifications"
  ],
  "host_permissions": [
    "https://suggestqueries.google.com/complete/search",
    "https://www.geonames.org/findNearbyPlaceName",
    "https://accounts.google.com/o/oauth2/v2/auth",
    "https://www.googleapis.com/calendar/v3/calendars",
    "https://reflect-newtab.app/*"
  ],
  "key": ""
}
